{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "member-ordering": false,
        "no-bitwise": false,
        "no-console": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "quotemark": [true, "single", "jsx-double"]
    },
    "rulesDirectory": []
}
